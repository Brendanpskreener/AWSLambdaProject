  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "vscode-jest-tests",
      "request": "launch",
      "args": [
        "--runInBand"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke Word Reverser",
      "program": "C:\\Users\\DEATH PONY\\AppData\\Roaming\\npm\\node_modules\\serverless\\bin\\serverless",
      "args": [
        "invoke",
        "local",
        "-f",
        "wordReverser",
        "-p",
        "json/local/wordReverser.event.json"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke Image Resizer",
      "program": "C:\\Users\\DEATH PONY\\AppData\\Roaming\\npm\\node_modules\\serverless\\bin\\serverless",
      "args": [
        "invoke",
        "local",
        "-f",
        "imageResizer"
      ],
      "internalConsoleOptions": "neverOpen",
      "console": "integratedTerminal"
    }
  ]
}
